                            <TextBox x:Name="PasportNomber" HorizontalAlignment="Left" Height="23" Margin="10,234,0,0" TextWrapping="Wrap" Text="Номер паспорта" VerticalAlignment="Top" Width="220" TextChanged="PasportNuberTextBox"/>
